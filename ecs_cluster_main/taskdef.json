

[
  {
    "name" : "${task_definition_name}",
    "image": "${docker_image_url}",
    "essential" : true,
    "portMappings": [
     {
       "containerPort": ${app_port},
       "hostPort": ${app_port}
     }
    ],
    "logConfiguration": {
         "logDriver": "awslogs",
         "options": {
           "awslogs-group": "${task_definition_name}-LogGroup",
           "awslogs-region": "${region}",
           "awslogs-stream-prefix": "ecs"
      }
    }
  }
]
